{
  "name": "remark-external-links",
  "version": "2.0.0",
  "description": "Automatically adds the target and rel attributes to external links",
  "license": "MIT",
  "keywords": [
    "markdown",
    "remark",
    "plugin",
    "link",
    "url"
  ],
  "repository": "remarkjs/remark-external-links",
  "bugs": "https://github.com/remarkjs/remark-external-links/issues",
  "author": "Cédric Delpoux <xuopled@gmail.com>",
  "contributors": [
    "Cédric Delpoux <xuopled@gmail.com>",
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "space-separated-tokens": "^1.1.2",
    "unist-util-visit": "^1.4.0"
  },
  "devDependencies": {
    "jest": "~23.5.0",
    "prettier": "^1.14.2",
    "remark": "~9.0.0",
    "remark-cli": "^5.0.0",
    "remark-html": "~8.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "xo": "^0.22.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "jest",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
